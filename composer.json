{
  "name": "reesmcivor/laravel-image-tools",
  "description": "A Laravel package for advanced image manipulation using Imagick.",
  "type": "library",
  "require": {
    "php": "^7.3|^8.0",
    "intervention/image": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "ReesMcIvor\\ImageTools\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ReesMcIvor\\ImageTools\\ImageToolsServiceProvider"
      ],
      "aliases": {
        "ImageTools": "ReesMcIvor\\ImageTools\\Facades\\ImageTools"
      }
    }
  }
}
